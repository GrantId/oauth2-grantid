{
  "name": "grant-software/oauth2-grantid",
  "description": "GrantId OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
  "license": "MIT",
  "authors": [
    {
      "name": "Grant Software",
      "email": "andrel@grantid.com",
      "homepage": "https://github.com/GrantId"
    }
  ],
  "keywords": [
    "oauth",
    "oauth2",
    "grantid",
    "client",
    "authorization",
    "authorisation"
  ],
  "require": {
    "php": ">=5.6.0",
    "league/oauth2-client": "^2.2.1"
  },
  "autoload": {
    "psr-4": {
      "GrantId\\OAuth2\\Client\\Provider\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GrantId\\OAuth2\\Client\\Test\\Provider\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "7",
    "jakub-onderka/php-parallel-lint": "^1.0",
    "phpunit/php-code-coverage": "^6.0"
  }
}
